#include "blueprint.fc";

contract test_contract_long {


    test initialize_large_state {
        uint256 large_value = 12345678901234567890;
        dict<slice, int> large_dict = ... ;
        slice address = <some_address>;

        () initialize_contract_with_large_state(large_value, address, large_dict);


        expect_state("balance", large_value);
        expect_state("address", address);
        expect_state("dict", large_dict);
    }

    
    test exchange_messages_between_contracts {
        slice contractA = <contractA_address>;
        slice contractB = <contractB_address>;
        cell message_body = begin_cell().store_uint(0x5678, 32).end_cell();

        
        () send_message(contractA, contractB, 500000, message_body, 0);


        expect_message_received(contractB, message_body);
    }
}
